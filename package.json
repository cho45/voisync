    "canvas": "^3.1.2",
    "openapi-fetch": "^0.14.0",
    "psd": "^3.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@types/node": "^24.1.0",
    "@vitest/browser": "^3.2.4",
    "openapi-typescript": "^7.8.0",
    "playwright": "^1.54.1",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  }
